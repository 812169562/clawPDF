<actionControls:ActionControl x:Class="clawSoft.clawPDF.Shared.Views.ActionControls.HttpUploadActionControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
            xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
            xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
            xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
            xmlns:actionControls="clr-namespace:clawSoft.clawPDF.Shared.Views.ActionControls"
            xmlns:userControls="clr-namespace:clawSoft.clawPDF.Shared.Views.UserControls"
            mc:Ignorable="d"
            x:Name="This">
    <StackPanel>

        <Label>
            <TextBlock Name="HttpServerText">HTTP服务地址:</TextBlock>
        </Label>
        <DockPanel>

            <TextBox Name="HttpServerTextBox" Text="{Binding CurrentProfile.HttpUploader.HttpUploadUrl}" />

        </DockPanel>

    </StackPanel>
</actionControls:ActionControl>
